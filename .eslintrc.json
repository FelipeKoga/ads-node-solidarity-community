{
  "env": {
    "browser": true,
    "es2020": true,
    "jest": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": [2, "always"],
    "space-before-function-paren": "off",
    "no-unused-vars": "off",
    "comma-dangle": "off"
  }
}
